<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <record id="seq_hms_patient" model="ir.sequence">
            <field name="name">Indimedi Patient</field>
            <field name="code">hms.patient</field>
            <field name="prefix">KH%(year)s%(month)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hms.appointment -->
        <record id="seq_hms_appointment" model="ir.sequence">
            <field name="name">Indimedi Appointment</field>
            <field name="code">hms.appointment</field>
            <field name="prefix">KH</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_indimedi_doctor" model="ir.sequence">
            <field name="name">Doctor</field>
            <field name="code">hms.physician</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

       <record id="account.sequence_payment_customer_invoice" model="ir.sequence">
            <field eval="6" name="padding"/>
            <field name="prefix">KH/%(range_y)s-17/</field>
        </record>

    </data>
</openerp>
